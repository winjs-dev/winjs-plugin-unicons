{
  "name": "@winner-fed/plugin-unicons",
  "version": "1.0.4",
  "repository": "https://github.com/winjs-dev/winjs-plugin-unicons",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "lint": "biome check .",
    "lint:write": "biome check . --write",
    "prepare": "simple-git-hooks && npm run build",
    "test": "playwright test",
    "bump": "npx bumpp"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint:write"
  },
  "dependencies": {
    "unplugin-icons": "^22.2.0",
    "svgo": "^3.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@playwright/test": "^1.54.2",
    "@rsbuild/core": "^1.4.15",
    "@rslib/core": "^0.11.2",
    "@types/node": "^22.17.1",
    "@winner-fed/winjs": "^0.16.10",
    "@winner-fed/utils": "^0.16.10",
    "playwright": "^1.54.2",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@rsbuild/core": "1.x",
    "@winner-fed/winjs": "^0.16.4",
    "@winner-fed/utils": "^0.16.4"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    },
    "@winner-fed/utils": {
      "optional": true
    }
  },
  "keywords": [
    "winjs",
    "plugin",
    "icons",
    "unicons",
    "svg",
    "vue",
    "vue2",
    "vue3",
    "vite",
    "webpack",
    "rspack",
    "rsbuild",
    "rslib",
    "unplugin-icons",
    "iconify",
    "icon-library",
    "icon-components",
    "auto-import",
    "tree-shaking",
    "svg-optimization",
    "local-icons",
    "custom-icons",
    "ui-components",
    "frontend",
    "build-tools",
    "bundler-plugin"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
